{
  "name": "angular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "f": "ng serve --configuration development",
    "lint": "ng lint",
    "prod": "ng build --configuration=production --output-path dist",
    "zip": "npm run prod -- --output-path=dist && gzipper c ./dist",
    "u": "ng update @angular/core@14 @angular/cli@14 --force --allow-dirty && ng update @angular/material@14  --force --allow-dirty",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@microsoft/signalr": "^3.1.5",
    "bootstrap-css-only": "^4.4.1",
    "moment": "^2.24.0",
    "ngx-toastr": "^12.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "xlsx": "^0.16.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "gzipper": "^7.2.0",
